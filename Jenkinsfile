pipeline {
    agent any
      stages{
         stage('Build'){
           steps{
             echo "Build"
             echo "success"
         }    
       }
         stage('test'){
           steps{
             echo "test"
             echo "integration success"
         }    
       }
         stage('integration Build'){
           steps{
             echo "integration final output"
         }    
       }
    }       
}

